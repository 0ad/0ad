#, c-format
msgid "Saved game to '%s'"
msgstr "Hra uložena do '%s'"

#: ps/Util.cpp:289 ps/Util.cpp:292 ps/Util.cpp:437 ps/Util.cpp:440
#, c-format
msgid "Screenshot written to '%s'"
msgstr "Snímek obrazovky zapsán do '%s'"

