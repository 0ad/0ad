msgstr "El jugador ya se encuentra conectado"

#: lobby/XmppClient.cpp:1204
msgid "Forbidden"
msgstr "Prohibido"

#: lobby/XmppClient.cpp:1206
msgid "Internal server error"
msgstr "Problema interno del servidor"

#: lobby/XmppClient.cpp:1210
msgid "Not allowed"
msgstr "No permitido "

#: lobby/XmppClient.cpp:1211
msgid "Not authorized"
msgstr "No autorizado"

#: lobby/XmppClient.cpp:1214
