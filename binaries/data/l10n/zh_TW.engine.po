msgid "Not allowed"
msgstr "不被允許"

#: lobby/XmppClient.cpp:1257
msgid "Not authorized"
msgstr "未授權"

#: lobby/XmppClient.cpp:1260
msgid "Recipient temporarily unavailable"
msgstr "暫時找不到接收者"

