#: lobby/XmppClient.cpp:1333
msgid "The certificate is not trusted."
msgstr "憑證未受信任"

#: lobby/XmppClient.cpp:1334
msgid "The certificate hasn't got a known issuer."
msgstr "憑證沒有已知的發行人"

#: lobby/XmppClient.cpp:1335
msgid "The certificate has been revoked."
msgstr "憑證已被吊銷"

#: lobby/XmppClient.cpp:1336
msgid "The certificate has expired."
msgstr "憑證已失效"

#: lobby/XmppClient.cpp:1337
msgid "The certificate is not yet active."
msgstr "憑證尚未被啟用"

#: lobby/XmppClient.cpp:1338
msgid "The certificate has not been issued for the peer connected to."
msgstr "憑證尚未被發出鄰機連接"

#: lobby/XmppClient.cpp:1339
msgid "The certificate signer is not a certificate authority."
msgstr "憑證簽署人非官方發證人"

#: lobby/XmppClient.cpp:1361 lobby/XmppClient.cpp:1405
#: lobby/XmppClient.cpp:1444
msgid "Error"
msgstr "錯誤"

#: lobby/XmppClient.cpp:1364 lobby/XmppClient.cpp:1408
msgid "No error"
msgstr "沒有錯誤"

#: lobby/XmppClient.cpp:1366
msgid "Player already logged in"
msgstr "玩家已登入"

#: lobby/XmppClient.cpp:1368
msgid "Forbidden"
msgstr "被拒絕"

#: lobby/XmppClient.cpp:1370
msgid "Internal server error"
msgstr "內部伺服器錯誤"

#: lobby/XmppClient.cpp:1374
msgid "Not allowed"
msgstr "不被允許"

#: lobby/XmppClient.cpp:1375
msgid "Not authorized"
msgstr "未授權"

#: lobby/XmppClient.cpp:1378
msgid "Recipient temporarily unavailable"
msgstr "暫時找不到接收者"

#: lobby/XmppClient.cpp:1380
msgid "Registration required"
msgstr "需要註冊"

#: lobby/XmppClient.cpp:1384
msgid "Service unavailable"
msgstr "服務不可用"

#: lobby/XmppClient.cpp:1389 lobby/XmppClient.cpp:1428
msgid "Unknown error"
msgstr "未知的錯誤"

#: lobby/XmppClient.cpp:1409
msgid "Stream error"
msgstr "串流錯誤"

#: lobby/XmppClient.cpp:1410
msgid "The incoming stream version is unsupported"
msgstr "不支援傳入的串流版本"

#: lobby/XmppClient.cpp:1411
msgid "The stream has been closed by the server"
msgstr "串流已由伺服器關閉"

#: lobby/XmppClient.cpp:1415
msgid "An I/O error occurred"
msgstr "一個 輸入/輸出 錯誤發生了"

#: lobby/XmppClient.cpp:1417
msgid "The connection was refused by the server"
msgstr "連線被伺服器拒絕"

#: lobby/XmppClient.cpp:1418
msgid "Resolving the server's hostname failed"
msgstr "解析伺服器主機名稱失敗"

#: lobby/XmppClient.cpp:1419
msgid "This system is out of memory"
msgstr "這個系統的記憶體已耗盡"

#: lobby/XmppClient.cpp:1421
msgid ""
"The server's certificate could not be verified or the TLS handshake did not "
"complete successfully"
msgstr "伺服器的憑證無法被驗證或是 TLS 交握並未成功完成"

#: lobby/XmppClient.cpp:1422
msgid "The server did not offer required TLS encryption"
msgstr "伺服器並未提供必須的 TLS 加密"

#: lobby/XmppClient.cpp:1424
msgid "Authentication failed. Incorrect password or account does not exist"
msgstr "認證失敗。不正確的密碼或是帳號不存在"

#: lobby/XmppClient.cpp:1425
msgid "The user or system requested a disconnect"
msgstr "使用者或系統請求斷線"

#: lobby/XmppClient.cpp:1426
msgid "There is no active connection"
msgstr "沒有活躍中的連線"

#: lobby/XmppClient.cpp:1447
msgid "Your account has been successfully registered"
msgstr "你的帳號已經成功地註冊了"

#: lobby/XmppClient.cpp:1448
msgid "Not all necessary information provided"
msgstr "未提供所有必須的資訊"

#: lobby/XmppClient.cpp:1449
msgid "Username already exists"
msgstr "使用者名稱已存在"

