msgid "unknown subtype (see logs)"
msgstr "sotipu desconocíu (ve rexistros)"

#: lobby/XmppClient.cpp:986 lobby/XmppClient.cpp:1030
#: lobby/XmppClient.cpp:1069
msgid "Error"
msgstr "Fallu"

#: lobby/XmppClient.cpp:989 lobby/XmppClient.cpp:1033
msgid "No error"
msgstr "Dengún fallu"

#: lobby/XmppClient.cpp:991
msgid "Player already logged in"
msgstr "El xugador yá anició sesión"

#: lobby/XmppClient.cpp:993
msgid "Forbidden"
msgstr "Prohibío"

#: lobby/XmppClient.cpp:995
msgid "Internal server error"
msgstr "Fallu internu del sirvidor"

#: lobby/XmppClient.cpp:999
msgid "Not allowed"
msgstr "Non permitío"

#: lobby/XmppClient.cpp:1000
msgid "Not authorized"
msgstr "Non autorizao"

#: lobby/XmppClient.cpp:1003
msgid "Recipient temporarily unavailable"
msgstr "Receutor non disponible temporalmente"

#: lobby/XmppClient.cpp:1005
msgid "Registration required"
msgstr "Inscripción requerida"

#: lobby/XmppClient.cpp:1009
msgid "Service unavailable"
msgstr "Serviciu non disponible"

#: lobby/XmppClient.cpp:1014 lobby/XmppClient.cpp:1053
msgid "Unknown error"
msgstr "Fallu desconocíu"

#: lobby/XmppClient.cpp:1034
msgid "Stream error"
msgstr "Fallu del fluxu"

#: lobby/XmppClient.cpp:1035
msgid "The incoming stream version is unsupported"
msgstr "Nun se sofita la versión del fluxu entrante"

#: lobby/XmppClient.cpp:1036
msgid "The stream has been closed by the server"
msgstr "El sirvidor zarró'l fluxu"

#: lobby/XmppClient.cpp:1040
msgid "An I/O error occured"
msgstr "Hebo un fallu E/S"

#: lobby/XmppClient.cpp:1042
msgid "The connection was refused by the server"
msgstr "El sirvidor refugó la conexón"

#: lobby/XmppClient.cpp:1043
msgid "Resolving the server's hostname failed"
msgstr "Falló la igua del nome d'equipu del sirvidor"

#: lobby/XmppClient.cpp:1044
msgid "This system is out of memory"
msgstr "Esti sistema ta ensin memoria"

#: lobby/XmppClient.cpp:1046
msgid ""
"The server's certificate could not be verified or the TLS handshake did not "
"complete successfully"
msgstr "O nun pudo comprobase'l certificáu del sirvidor o nun se completó con ésitu'l handshake TLS"

#: lobby/XmppClient.cpp:1047
msgid "The server did not offer required TLS encryption"
msgstr "El sirvidor nun presentó'l cifráu TLS requeríu"

#: lobby/XmppClient.cpp:1049
msgid "Authentication failed. Incorrect password or account does not exist"
msgstr "Falló l'autenticación. Nun esiste la cuenta o la clave incorreuta"

#: lobby/XmppClient.cpp:1050
msgid "The user or system requested a disconnect"
msgstr "L'usuariu o'l sistema pidió desconectase"

#: lobby/XmppClient.cpp:1051
msgid "There is no active connection"
msgstr "Nun hai conexón activa"

#: lobby/XmppClient.cpp:1072
msgid "Success"
msgstr "Ésitu"

#: lobby/XmppClient.cpp:1073
msgid "Not all necessary information provided"
msgstr "Nun s'apurrió tola información necesario"

#: lobby/XmppClient.cpp:1074
msgid "Username already exists"
msgstr "Yá esiste'l nome d'usuariu"

