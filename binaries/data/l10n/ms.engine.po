#: ps/ModIo.cpp:293
#, c-format
msgid "Failure while starting querying for mods. Error: %s; %s."
msgstr "Gagal ketika mula meminta mod. Ralat: %s; %s."

#: ps/ModIo.cpp:319
#, c-format
msgid "Could not create mod directory: %s."
msgstr "Tidak dapat mencipta direktori mod: %s."

#: ps/ModIo.cpp:344
#, c-format
