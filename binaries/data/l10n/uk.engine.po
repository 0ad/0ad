msgid "Error"
msgstr "Помилка"

#: lobby/XmppClient.cpp:1013 lobby/XmppClient.cpp:1057
msgid "No error"
msgstr "Без помилки"

#: lobby/XmppClient.cpp:1015
msgid "Player already logged in"
msgstr "Гравець вже увійшов"

#: lobby/XmppClient.cpp:1017
msgid "Forbidden"
msgstr "Заборонено"

#: lobby/XmppClient.cpp:1019
msgid "Internal server error"
msgstr "Внутрішня помилка сервера"

#: lobby/XmppClient.cpp:1023
msgid "Not allowed"
msgstr "Забороняється"

#: lobby/XmppClient.cpp:1024
msgid "Not authorized"
msgstr "Не авторизовано"

#: lobby/XmppClient.cpp:1027
msgid "Recipient temporarily unavailable"
msgstr "Одержувач тимчасово не доступний"

#: lobby/XmppClient.cpp:1029
msgid "Registration required"
msgstr "Потрібна реєстрація"

#: lobby/XmppClient.cpp:1033
msgid "Service unavailable"
msgstr "Сервіс недоступний"

#: lobby/XmppClient.cpp:1038 lobby/XmppClient.cpp:1077
msgid "Unknown error"
msgstr "Невідома помилка"

#: lobby/XmppClient.cpp:1058
msgid "Stream error"
msgstr "Помилка потоку"

#: lobby/XmppClient.cpp:1059
msgid "The incoming stream version is unsupported"
msgstr "Версія вхідного потоку не підтримується"

#: lobby/XmppClient.cpp:1060
msgid "The stream has been closed by the server"
msgstr "Потік закритий сервером"

#: lobby/XmppClient.cpp:1064
msgid "An I/O error occurred"
msgstr ""

#: lobby/XmppClient.cpp:1066
msgid "The connection was refused by the server"
msgstr "З'єднання розірвано сервером"

#: lobby/XmppClient.cpp:1067
msgid "Resolving the server's hostname failed"
msgstr "Вирішення назв вузлів сервера не вдалося"

#: lobby/XmppClient.cpp:1068
msgid "This system is out of memory"
msgstr "Цій системі бракує пам'яті"

#: lobby/XmppClient.cpp:1070
msgid ""
"The server's certificate could not be verified or the TLS handshake did not "
"complete successfully"
msgstr "Сертифікат сервера не можна перевірити або TLS handshake не вдалося успішно завершити"

#: lobby/XmppClient.cpp:1071
msgid "The server did not offer required TLS encryption"
msgstr "Сервер не запропонував потрібне шифрування TLS"

#: lobby/XmppClient.cpp:1073
msgid "Authentication failed. Incorrect password or account does not exist"
msgstr "Вхід не вдався. Неправильний пароль або профіль не існує"

#: lobby/XmppClient.cpp:1074
msgid "The user or system requested a disconnect"
msgstr "Користувач або система попросили роз'єднатися"

#: lobby/XmppClient.cpp:1075
msgid "There is no active connection"
msgstr "Не має активного з'єднання"

#: lobby/XmppClient.cpp:1096
msgid "Your account has been successfully registered"
msgstr ""

#: lobby/XmppClient.cpp:1097
msgid "Not all necessary information provided"
msgstr "Не надано усіх необхідних даних"

#: lobby/XmppClient.cpp:1098
msgid "Username already exists"
msgstr "Користувач з таким ім'ям вже існує"

