#: i18n/L10n.cpp:251
msgid "Long strings"
msgstr "Երկար տողեր"

#: lobby/XmppClient.cpp:739
msgid "unknown subtype (see logs)"
msgstr "Անհայտ ենթատեսակ (տես մուտքերում)"

#: lobby/XmppClient.cpp:1003 lobby/XmppClient.cpp:1047
#: lobby/XmppClient.cpp:1086
msgid "Error"
msgstr "Սխալ"

#: lobby/XmppClient.cpp:1006 lobby/XmppClient.cpp:1050
msgid "No error"
msgstr "Ոչ մի սխալ"

#: lobby/XmppClient.cpp:1008
msgid "Player already logged in"
msgstr "Խաղացողն արդեն մուտք է գործել"

#: lobby/XmppClient.cpp:1010
msgid "Forbidden"
msgstr "Անթույլատրելի"

#: lobby/XmppClient.cpp:1012
msgid "Internal server error"
msgstr "Ներքին սերվերի սխալ"

#: lobby/XmppClient.cpp:1016
msgid "Not allowed"
msgstr "Ոչ թույլատրված"

#: lobby/XmppClient.cpp:1017
msgid "Not authorized"
msgstr "Ոչ հաստատված"

#: lobby/XmppClient.cpp:1020
msgid "Recipient temporarily unavailable"
msgstr "Ստացողը ժամանակավորապես անհասանելի է"

#: lobby/XmppClient.cpp:1022
msgid "Registration required"
msgstr "Անհրաժեշտ է գրանցում"

#: lobby/XmppClient.cpp:1026
msgid "Service unavailable"
msgstr "Ծառայությունն անհասանելի է"

#: lobby/XmppClient.cpp:1031 lobby/XmppClient.cpp:1070
msgid "Unknown error"
msgstr "Անհայտ սխալ"

#: lobby/XmppClient.cpp:1051
msgid "Stream error"
msgstr "Հոսքի սխալ"

#: lobby/XmppClient.cpp:1052
msgid "The incoming stream version is unsupported"
msgstr "Ժամանող հոսքի տարբերակը չի ապահովվում"

#: lobby/XmppClient.cpp:1053
msgid "The stream has been closed by the server"
msgstr "Հոսքը փակվել է սերվերի կողմից"

#: lobby/XmppClient.cpp:1057
msgid "An I/O error occurred"
msgstr ""

#: lobby/XmppClient.cpp:1059
msgid "The connection was refused by the server"
msgstr "Կապը չեղարկվել է սերվերի կողմից"

#: lobby/XmppClient.cpp:1060
msgid "Resolving the server's hostname failed"
msgstr "Սերվերի անվանման ստացումը չհաջողվեց"

#: lobby/XmppClient.cpp:1061
msgid "This system is out of memory"
msgstr "Համակարգն ազատ հիշողություն չունի"

#: lobby/XmppClient.cpp:1063
msgid ""
"The server's certificate could not be verified or the TLS handshake did not "
"complete successfully"
msgstr "Սերվերի վկայականն անհնար է հաստատել կամ TLS միացումը չի հաջողվել"

#: lobby/XmppClient.cpp:1064
msgid "The server did not offer required TLS encryption"
msgstr "Սերվերը չառաջարկեց անհրաժեշտ TLS կոդավորումը"

#: lobby/XmppClient.cpp:1066
msgid "Authentication failed. Incorrect password or account does not exist"
msgstr "Նույնականացման ձախողում։ Գաղտնաբառը սխալ է կամ հաշիվը գոյություն չունի"

#: lobby/XmppClient.cpp:1067
msgid "The user or system requested a disconnect"
msgstr "Օգտատերը կամ սերվերը պահանջել են անջատում"

#: lobby/XmppClient.cpp:1068
msgid "There is no active connection"
msgstr "Ակտիվ կապ չկա"

#: lobby/XmppClient.cpp:1089
msgid "Success"
msgstr "Հաջողություն"

#: lobby/XmppClient.cpp:1090
msgid "Not all necessary information provided"
msgstr "Անհրաժեշտ տեղեկությունը տրամադրված է ոչ ամբողջությամբ"

#: lobby/XmppClient.cpp:1091
msgid "Username already exists"
msgstr "Օգտանունն արդեն գոյություն ունի"

#: ps/SavedGame.cpp:133
#, c-format
msgid "Saved game to '%s'"
msgstr "Խաղը պահպանվեց '%s'-ում"

#: ps/Util.cpp:279 ps/Util.cpp:282 ps/Util.cpp:417 ps/Util.cpp:420
#, c-format
msgid "Screenshot written to '%s'"
msgstr "Էկրանակադրը պահպանվեց '%s'-ում"

