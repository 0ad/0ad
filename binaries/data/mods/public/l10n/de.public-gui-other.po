msgstr "Ungeordnet"

#: gui/options/options.json[6].options[14].list[1].label
msgid "Ascending"
msgstr "Aufsteigend"

#: gui/options/options.json[6].options[14].list[2].label
msgid "Descending"
msgstr "Absteigend"

