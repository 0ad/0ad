msgid "Indibil"
msgstr "Індібіл"

#: simulation/data/civs/iber.jsonAINames[4]
msgid "Audax"
msgstr "Аудакс"

#: simulation/data/civs/iber.jsonAINames[5]
msgid "Ditalcus"
msgstr "Діталк"

#: simulation/data/civs/iber.jsonAINames[6]
