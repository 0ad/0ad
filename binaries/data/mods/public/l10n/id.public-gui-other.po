msgstr "Antialias lambat, tetapi berkualitas tinggi, menggunakan delapan sampel per piksel. Didukung untuk GL3.3+."

#: gui/options/options.json[1].options[4].list[5].label
msgid "MSAA (16×)"
msgstr "MSAA (16×)"

#: gui/options/options.json[1].options[4].list[5].tooltip
