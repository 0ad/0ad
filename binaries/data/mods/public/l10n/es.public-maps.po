#: maps/scenarios/the_persian_gates.xml:PlayerData[2].Name:26
msgid "Ariobarzanes"
msgstr "Ariobarzanes"

#: maps/scenarios/the_persian_gates.xml:PlayerData[3].Name:26
msgid "Persians"
msgstr "Persas"

