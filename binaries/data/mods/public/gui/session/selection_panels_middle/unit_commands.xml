<?xml version="1.0" encoding="UTF-8"?>
<!-- Unit Commands -->
<object name="unitCommandPanel"
	size="0 100%-40 100% 100%-4"
	type="image"
	z="30"
>
	<object size="0 0 100% 100%">
		<repeat count="6">
			<object name="unitCommandButton[n]" hidden="true" style="iconButton" type="button" size="0 0 32 32" tooltip_style="sessionToolTipBottomBold">
				<object name="unitCommandIcon[n]" ghost="true" type="image" style="commandIcon"/>
				<object name="unitCommandCount[n]" ghost="true" style="groupIconsText" type="text"/>
			</object>
		</repeat>
	</object>
</object>
