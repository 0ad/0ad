<?xml version="1.0" encoding="UTF-8"?>
<object name="unitGroupPanel"
	size="0 50%-228 38 50%+152"
>
	<repeat count="10">
		<object name="unitGroupButton[n]" size="0 0 38 38" type="button" style="iconButton" tooltip_style="sessionToolTipBottomBold">
			<translatableAttribute id="tooltip">Click to select grouped units, double-click to focus the grouped units and right-click to disband the group.</translatableAttribute>
			<object name="unitGroupIcon[n]" size="3 3 35 35" type="image" sprite="groupsIcon" ghost="true"/>
			<object name="unitGroupLabel[n]" type="text" style="largeCenteredOutlinedText" ghost="true"/>
		</object>
	</repeat>
</object>
