    <uniform name="velocity.y" min="-0.25" max="0.25"/>
    <uniform name="velocity.z" min="-1.0" max="-1.0"/>
    <uniform name="velocity.angle" min="-1.5" max="1.5"/>

    <uniform name="size" min="0.5" max="0.75"/>

    <force y="-1.50"/>

