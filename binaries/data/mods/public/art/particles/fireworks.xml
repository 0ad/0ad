<?xml version="1.0" encoding="utf-8"?>
<particles>

    <texture>art/textures/particles/fireworks.png</texture>
    <blend mode="add"/>

    <constant name="emissionrate" value="20"/>
    <uniform name="lifetime" min="10.0" max="20.5"/>
    
	
    <uniform name="angle" min="-0.5" max="0.5"/>

    <uniform name="velocity.x" min="-3" max="3"/>
    <uniform name="velocity.y" min="10" max="10.5"/>
    <uniform name="velocity.z" min="-3" max="3"/>
    <uniform name="velocity.angle" min="-0.5" max="0.5"/>

    <uniform name="size" min="0.35" max="0.6"/>

    <uniform name="color.r" min="0.1" max="0.9"/>
	<uniform name="color.g" min="0.1" max="0.9"/>
	<uniform name="color.b" min="0.1" max="0.9"/>

	

	<force y="-2.5"/>


</particles>
